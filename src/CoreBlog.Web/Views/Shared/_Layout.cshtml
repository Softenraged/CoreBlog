@inject Microsoft.Extensions.Configuration.IConfigurationRoot config
<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootswatch/readable/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css" />
    @RenderSection("Styles", false)
</head>

<body>

    <div class="layout">
        <div>
            @Html.Partial("_NavBar")
        </div>
        <div class="content">
            @RenderBody()
        </div>
        @Html.Partial("_Footer")
    </div>


    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    @RenderSection("Scripts", false)
    <script src="https://www.google.com/recaptcha/api.js"></script>
    <script id="dsq-count-scr" src="//@(config["Site:Disqus"])coreblogapp.disqus.com/count.js" async></script>
</body>
</html>