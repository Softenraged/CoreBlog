@model ICollection<Blog.ViewModels.PostViewModel>
@{
    ViewBag.Title = "Manage panel";
}


<ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#home">New posts</a></li>
    <li><a data-toggle="tab" href="#menu1">Users</a></li>
    <li><a data-toggle="tab" href="#menu2">Suspended Users</a></li>
</ul>

<div class="tab-content">
    <div id="home" class="tab-pane fade in active">
        <table class="table">
            @foreach (var post in Model)
            {
                <tr>
                    <td>@post.Title</td>
                    <td>@post.PostedOn</td>
                    <td>@post.Author</td>
               </tr>

            }
        </table>
    </div>
    <div id="menu1" class="tab-pane fade">
        <h3>Menu 1</h3>
        <p>Some content in menu 1.</p>
    </div>
    <div id="menu2" class="tab-pane fade">
        <h3>Menu 2</h3>
        <p>Some content in menu 2.</p>
    </div>
</div>